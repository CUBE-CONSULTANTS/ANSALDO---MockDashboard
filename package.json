{
  "name": "ansaldonucleardashboard",
  "version": "1.0.0",
  "description": "UI5 Application: ansaldonucleardashboard",
  "license": "Apache-2.0",
  "scripts": {
    "clean": "rimraf dist coverage",
    "build": "ui5 build --clean-dest",
    "build:opt": "ui5 build self-contained --clean-dest --all",
    "build:cf": "ui5 build preload --clean-dest --config ui5-deploy.yaml --include-task=generateCachebusterInfo",
    "deploy": "npm run build && fiori deploy --config ui5-deploy.yaml --verbose && rimraf archive.zip",
    "deploy:nwabap": "npm run build && nwabap upload --nwabaprc .nwabaprc",
    "start": "ui5 serve --port 8000 -o index.html",
    "start-cdn": "ui5 serve --port 8000 -o index-cdn.html",
    "start:dist": "npm start -- --config ui5-dist.yaml",
    "start:dist-cdn": "npm run start-cdn -- --config ui5-dist.yaml",
    "lint": "eslint webapp",
    "karma": "karma start",
    "karma-ci": "karma start karma-ci.conf.js",
    "karma-ci-cov": "karma start karma-ci-cov.conf.js",
    "test": "npm run lint && npm run karma-ci-cov",
    "wdi5": "wdio run ./webapp/test/e2e/\\wdio.conf.js"
  },
  "devDependencies": {
    "@sap/cds-dk": "^9",
    "@sap/ui5-builder-webide-extension": "^1.1.8",
    "@ui5/cli": "^4.0.6",
    "@ui5/middleware-code-coverage": "^2.0.0",
    "@wdio/cli": "^8.46.0",
    "@wdio/local-runner": "^8.46.0",
    "@wdio/mocha-framework": "^8.46.0",
    "@wdio/spec-reporter": "^8.43.0",
    "eslint": "^9.9.1",
    "eslint-plugin-jsdoc": "^50.2.2",
    "globals": "^15.9.0",
    "karma": "^6.4.4",
    "karma-chrome-launcher": "^3.2.0",
    "karma-coverage": "^2.2.1",
    "karma-ui5": "^4.0.1",
    "mbt": "^1.2.18",
    "nwabap-ui5uploader": "^1.2.0",
    "rimraf": "^6.0.1",
    "ui5-middleware-livereload": "^3.0.3",
    "ui5-task-nwabap-deployer": "^2.1.1",
    "ui5-task-zipper": "^0.5.0",
    "wdio-ui5-service": "^2.2.1"
  },
  "dependencies": {
    "@sap/ux-ui5-tooling": "^1.19.0",
    "@sap/xssec": "^4"
  },
  "cds": {
    "requires": {
      "[production]": {
        "auth": "xsuaa"
      },
      "connectivity": true,
      "destinations": true,
      "html5-repo": true
    }
  }
}
