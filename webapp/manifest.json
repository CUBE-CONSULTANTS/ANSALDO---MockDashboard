{
	"_version": "1.12.0",

	"sap.app": {
		"id": "ansaldonuclear.dashboard",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"title": "{{appTitle}}",
		"description": "{{appDescription}}",
		"applicationVersion": {
			"version": "${version}"
		},
		"sourceTemplate": {
			"id": "generator-ui5-app",
			"version": "1.1.0"
		}
	},

	"sap.ui": {
		"technology": "UI5",
		"icons": {},
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

	"sap.ui5": { 
		"flexEnabled": false,
    "contentDensities": {"compact": true, "cozy": true},
		"rootView": {
			"viewName": "ansaldonuclear.dashboard.view.App",
			"type": "XML",
			"id": "app"
		},

		"dependencies": {
			"minUI5Version": "1.140.0",
			"libs": {
				"sap.ui.core": {},
				"sap.m": {}
			}
		},

		"handleValidation": true,

		"models": {
			"i18n": {
				"type": "sap.ui.model.resource.ResourceModel",
				"settings": {
					"bundleName": "ansaldonuclear.dashboard.i18n.i18n",
					"supportedLocales": ["en", "de"],
					"fallbackLocale": "en"
				}
			}
		},
		"resources": {
			"css": [
			  {
				"uri": "css/style.css"
			  }
			]
		},
		"routing": {
			"config": {
				"routerClass": "sap.m.routing.Router",
				"viewType": "XML",
				"viewPath": "ansaldonuclear.dashboard.view",
				"controlId": "app",
				"controlAggregation": "pages"
			},
			"routes": [
				{
					"pattern": "",
					"name": "main",
					"target": "main"
				},
				{
					"pattern": "HrTimesheet",
					"name": "HrTimesheet",
					"target": "HrTimesheet"
				},
				{
					"pattern": "Manufacturing",
					"name": "Manufacturing",
					"target": "Manufacturing"
				},
				{
					"pattern": "Finance",
					"name": "Finance",
					"target": "Finance"
				},
				{
					"pattern": "ProjectSystem",
					"name": "ProjectSystem",
					"target": "ProjectSystem"
				}
			],
			"targets": {
				"main": {
					"viewId": "main",
					"viewName": "Main"
				},
				"HrTimesheet": {
					"viewId": "HrTimesheet",
					"viewName": "HrTimesheet"
				},
				"Manufacturing": {
					"viewId": "Manufacturing",
					"viewName": "Manufacturing"
				},
				"Finance": {
					"viewId": "Finance",
					"viewName": "Finance"
				},
				"ProjectSystem": {
					"viewId": "ProjectSystem",
					"viewName": "ProjectSystem"
				}
				
			}
		}
	}
}
